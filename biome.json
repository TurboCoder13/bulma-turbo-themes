{
  "linter": {
    "enabled": true,
    "rules": {
      "style": {
        "noNonNullAssertion": "off",
        "useImportType": "off",
        "useNodejsImportProtocol": "off",
        "useTemplate": "off"
      },
      "suspicious": {
        "noExplicitAny": "off",
        "noImplicitAnyLet": "off",
        "useIterableCallbackReturn": "off",
        "noGlobalIsNan": "off"
      },
      "correctness": {
        "noUnusedImports": "off",
        "noUnusedFunctionParameters": "off"
      }
    }
  }
}
