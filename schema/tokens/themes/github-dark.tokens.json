{
  "$schema": "../../turbo-themes.schema.json#/$defs/ThemeFile",
  "id": "github-dark",
  "label": "GitHub Dark",
  "vendor": "github",
  "appearance": "dark",
  "tokens": {
    "background": {
      "base": {
        "$value": "#0d1117",
        "$type": "color"
      },
      "surface": {
        "$value": "#151b23",
        "$type": "color"
      },
      "overlay": {
        "$value": "#010409",
        "$type": "color"
      }
    },
    "text": {
      "primary": {
        "$value": "#f0f6fc",
        "$type": "color"
      },
      "secondary": {
        "$value": "#9198a1",
        "$type": "color"
      },
      "inverse": {
        "$value": "#ffffff",
        "$type": "color"
      }
    },
    "brand": {
      "primary": {
        "$value": "#1f6feb",
        "$type": "color"
      }
    },
    "state": {
      "info": {
        "$value": "#4493f8",
        "$type": "color"
      },
      "success": {
        "$value": "#3fb950",
        "$type": "color"
      },
      "warning": {
        "$value": "#d29922",
        "$type": "color"
      },
      "danger": {
        "$value": "#f85149",
        "$type": "color"
      },
      "successText": {
        "$value": "#0d1117",
        "$type": "color"
      },
      "warningText": {
        "$value": "#0d1117",
        "$type": "color"
      }
    },
    "border": {
      "default": {
        "$value": "#3d444d",
        "$type": "color"
      }
    },
    "accent": {
      "link": {
        "$value": "#4493f8",
        "$type": "color"
      }
    },
    "typography": {
      "fonts": {
        "sans": {
          "$value": "\"Mona Sans\", -apple-system, BlinkMacSystemFont, \"Segoe UI\", \"Noto Sans\", Helvetica, Arial, sans-serif, \"Apple Color Emoji\", \"Segoe UI Emoji\"",
          "$type": "fontFamily"
        },
        "mono": {
          "$value": "\"Hubot Sans\", ui-monospace, SFMono-Regular, SF Mono, Menlo, Consolas, \"Liberation Mono\", monospace",
          "$type": "fontFamily"
        }
      },
      "webFonts": [
        "https://github.githubassets.com/assets/mona-sans-webfont.woff2",
        "https://github.githubassets.com/assets/hubot-sans-webfont.woff2"
      ]
    },
    "content": {
      "heading": {
        "h1": {
          "$value": "#3fb950",
          "$type": "color"
        },
        "h2": {
          "$value": "#4493f8",
          "$type": "color"
        },
        "h3": {
          "$value": "#1f6feb",
          "$type": "color"
        },
        "h4": {
          "$value": "#d29922",
          "$type": "color"
        },
        "h5": {
          "$value": "#3fb950",
          "$type": "color"
        },
        "h6": {
          "$value": "#f85149",
          "$type": "color"
        }
      },
      "body": {
        "primary": {
          "$value": "#f0f6fc",
          "$type": "color"
        },
        "secondary": {
          "$value": "#9198a1",
          "$type": "color"
        }
      },
      "link": {
        "default": {
          "$value": "#4493f8",
          "$type": "color"
        }
      },
      "selection": {
        "fg": {
          "$value": "#f0f6fc",
          "$type": "color"
        },
        "bg": {
          "$value": "#264f78",
          "$type": "color"
        }
      },
      "blockquote": {
        "border": {
          "$value": "#3d444d",
          "$type": "color"
        },
        "fg": {
          "$value": "#9198a1",
          "$type": "color"
        },
        "bg": {
          "$value": "#151b23",
          "$type": "color"
        }
      },
      "codeInline": {
        "fg": {
          "$value": "#f0f6fc",
          "$type": "color"
        },
        "bg": {
          "$value": "#151b23",
          "$type": "color"
        }
      },
      "codeBlock": {
        "fg": {
          "$value": "#f0f6fc",
          "$type": "color"
        },
        "bg": {
          "$value": "#151b23",
          "$type": "color"
        }
      },
      "table": {
        "border": {
          "$value": "#3d444d",
          "$type": "color"
        },
        "stripe": {
          "$value": "#151b23",
          "$type": "color"
        },
        "theadBg": {
          "$value": "#151b23",
          "$type": "color"
        }
      }
    }
  }
}
