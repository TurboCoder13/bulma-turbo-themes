  "label": "Gruvbox Light Hard",
  "vendor": "gruvbox",
  "appearance": "light",
  "iconUrl": "/assets/img/gruvbox-light-hard.png",
  "tokens": {
    "background": {
      "base": {
        "$value": "#f9f5d7",
        "$type": "color"
      },
      "surface": {
        "$value": "#fbf1c7",
        "$type": "color"
      },
      "overlay": {
        "$value": "#ebdbb2",
        "$type": "color"
      }
    },
    "text": {
      "primary": {
        "$value": "#3c3836",
        "$type": "color"
      },
      "secondary": {
        "$value": "#504945",
        "$type": "color"
      },
      "inverse": {
        "$value": "#f9f5d7",
        "$type": "color"
      }
    },
    "brand": {
      "primary": {
        "$value": "#b57614",
        "$type": "color"
      }
    },
    "state": {
      "info": {
        "$value": "#076678",
        "$type": "color"
      },
      "success": {
        "$value": "#79740e",
        "$type": "color"
      },
      "warning": {
        "$value": "#b57614",
        "$type": "color"
      },
      "danger": {
        "$value": "#9d0006",
