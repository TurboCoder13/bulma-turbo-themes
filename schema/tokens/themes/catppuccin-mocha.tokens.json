          "$value": "#cdd6f4",
          "$type": "color"
        },
        "bg": {
          "$value": "#313244",
          "$type": "color"
        }
      },
      "codeBlock": {
        "fg": {
          "$value": "#cdd6f4",
          "$type": "color"
        },
        "bg": {
          "$value": "#313244",
          "$type": "color"
        }
      },
      "table": {
        "border": {
          "$value": "#7f849c",
          "$type": "color"
        },
        "stripe": {
          "$value": "#313244",
          "$type": "color"
        },
        "theadBg": {
          "$value": "#45475a",
          "$type": "color"
        }
      }
    }
