{
  "$schema": "../../turbo-themes.schema.json#/$defs/ThemeFile",
  "id": "tokyo-night-storm",
  "label": "Storm",
  "vendor": "tokyo-night",
  "appearance": "dark",
  "iconUrl": "/assets/img/tokyo-night.png",
  "tokens": {
    "background": {
      "base": { "$value": "#24283b", "$type": "color" },
      "surface": { "$value": "#28344a", "$type": "color" },
      "overlay": { "$value": "#565f89", "$type": "color" }
    },
    "text": {
      "primary": { "$value": "#c0caf5", "$type": "color" },
      "secondary": { "$value": "#a9b1d6", "$type": "color" },
      "inverse": { "$value": "#24283b", "$type": "color" }
    },
    "brand": {
      "primary": { "$value": "#7aa2f7", "$type": "color" }
    },
    "state": {
      "info": { "$value": "#7dcfff", "$type": "color" },
      "success": { "$value": "#9ece6a", "$type": "color" },
      "warning": { "$value": "#e0af68", "$type": "color" },
      "danger": { "$value": "#f7768e", "$type": "color" }
    },
    "border": {
      "default": { "$value": "#565f89", "$type": "color" }
    },
    "accent": {
      "link": { "$value": "#7aa2f7", "$type": "color" }
    },
    "typography": {
      "fonts": {
        "sans": {
          "$value": "Inter, ui-sans-serif, system-ui, -apple-system, Segoe UI, Roboto, Helvetica, Arial, \"Apple Color Emoji\", \"Segoe UI Emoji\"",
          "$type": "fontFamily"
        },
        "mono": {
          "$value": "JetBrains Mono, ui-monospace, SFMono-Regular, Menlo, Monaco, Consolas, \"Liberation Mono\", \"Courier New\", monospace",
          "$type": "fontFamily"
        }
      },
      "webFonts": [
        "https://fonts.googleapis.com/css2?family=Inter:wght@400;600;700&display=swap",
        "https://fonts.googleapis.com/css2?family=JetBrains+Mono:wght@400;600;700&display=swap"
      ]
    },
    "content": {
      "heading": {
        "h1": { "$value": "#7aa2f7", "$type": "color" },
        "h2": { "$value": "#bb9af7", "$type": "color" },
        "h3": { "$value": "#7dcfff", "$type": "color" },
        "h4": { "$value": "#e0af68", "$type": "color" },
        "h5": { "$value": "#ff9e64", "$type": "color" },
        "h6": { "$value": "#f7768e", "$type": "color" }
      },
      "body": {
        "primary": { "$value": "#c0caf5", "$type": "color" },
        "secondary": { "$value": "#a9b1d6", "$type": "color" }
      },
      "link": {
        "default": { "$value": "#7aa2f7", "$type": "color" }
      },
      "selection": {
        "fg": { "$value": "#c0caf5", "$type": "color" },
        "bg": { "$value": "#28344a", "$type": "color" }
      },
      "blockquote": {
        "border": { "$value": "#565f89", "$type": "color" },
        "fg": { "$value": "#a9b1d6", "$type": "color" },
        "bg": { "$value": "#28344a", "$type": "color" }
      },
      "codeInline": {
        "fg": { "$value": "#c0caf5", "$type": "color" },
        "bg": { "$value": "#28344a", "$type": "color" }
      },
      "codeBlock": {
        "fg": { "$value": "#c0caf5", "$type": "color" },
        "bg": { "$value": "#28344a", "$type": "color" }
      },
      "table": {
        "border": { "$value": "#565f89", "$type": "color" },
        "stripe": { "$value": "#28344a", "$type": "color" },
        "theadBg": { "$value": "#28344a", "$type": "color" }
      }
    }
  }
}
